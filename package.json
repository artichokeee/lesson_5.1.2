{
  "name": "lesson_5.1.2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "cypress run",
    "xray:report": "curl -H \"Content-Type: text/xml\" -X POST -H \"Authorization: $your_token\" --data @\"test-results.xml\" https://xray.cloud.getxray.app/api/v2/import/execution/junit?projectKey=CYP",
    "cy:dev_dash": "cypress run --record --key 7f8b0f8a-dfaa-4e09-b154-d20485f64044",
    "cy:stage_dash": "cypress run --config-file cypress.stage.js --record --key 7f8b0f8a-dfaa-4e09-b154-d20485f64044",
    "cy:dev:chrome": "cypress run --config-file cypress.config.js --browser chrome",
    "cy:stage:electron": "cypress run --config-file cypress.stage.js --browser electron",
    "cy:dev_dash:chrome": "npm run cy:dev:chrome -- --record --key 7f8b0f8a-dfaa-4e09-b154-d20485f64044",
    "cy:stage_dash:electron": "npm run cy:stage:electron -- --record --key 7f8b0f8a-dfaa-4e09-b154-d20485f64044",
    "cy:dev_stage_dash:chrome:electron": "npm run cy:dev_dash:chrome && npm run cy:stage_dash:electron",
    "cy:run:cloud": "cypress run --record --key 7f8b0f8a-dfaa-4e09-b154-d20485f64044",
    "cy:spec1": "cypress run --spec 'cypress/e2e/3 - authorization/*' --record --key 7f8b0f8a-dfaa-4e09-b154-d20485f64044",
    "cy:spec2": "cypress run --spec 'cypress/e2e/2 - header menu/*' --record --key 7f8b0f8a-dfaa-4e09-b154-d20485f64044",
    "cy:nokey": "npm run cy:spec1 & npm run cy:spec2",
    "cy:dash": "npm run cy:spec1 & npm run cy:spec2",
    "cy:parallel": "npm run cy:spec1 --config-file cypress.laptop.js  -- --browser chrome --group chromeLaptop --ci-build-id 5 --parallel & npm run cy:spec2 --config-file cypress.monitor.js -- --browser electron --group electronMonitor --ci-build-id 5 --parallel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artichokeee/lesson_5.1.2.git"
  },
  "keywords": [
    "cypress"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artichokeee/lesson_5.1.2/issues"
  },
  "homepage": "https://github.com/artichokeee/lesson_5.1.2#readme",
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "cypress": "^13.6.1",
    "mocha-junit-reporter": "^2.2.1"
  }
}
